{
  "name": "lighthouse-plugin-greenhouse",
  "version": "0.0.2",
  "description": "A plugin for lighthouse to check for domains using green power",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "mobile-run": "lighthouse --plugins=lighthouse-plugin-greenhouse --view --chrome-flags='--headless' --output-path=./results/mobile.html",
    "json-run": "lighthouse --plugins=lighthouse-plugin-greenhouse --chrome-flags='--headless' --output=json --output-path=./report.json"
  },
  "keywords": [
    "tgwf"
  ],
  "author": "Chris Adams",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.19.0",
    "debug": "^4.1.1",
    "lodash.sortby": "^4.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "jest": "^24.8.0",
    "lighthouse": "^5.1.0",
    "np": "^5.0.3"
  },
  "peerDependencies": {
    "lighthouse": "^5.1.0"
  }
}
